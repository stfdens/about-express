{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "no-duplicate-imports": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "off",
        "max-len": "off",
        "object-curly-newline": "off",
        "no-unused-expressions": "off",
        "no-sequences": "off"
}}
